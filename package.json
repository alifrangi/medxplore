{
  "name": "medxplore",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "description": "MedXplore - Medical Excellence & Innovation Platform",
  "keywords": [
    "medical",
    "students",
    "healthcare",
    "education",
    "events"
  ],
  "author": "MedXplore Team",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:analyze": "ANALYZE=true vite build",
    "preview": "vite preview",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist",
    "build:prod": "npm run clean && npm run build",
    "serve": "npm run build && npm run preview"
  },
  "dependencies": {
    "canvas-confetti": "^1.9.3",
    "firebase": "^11.10.0",
    "framer-motion": "^10.16.16",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@vitejs/plugin-react": "^4.2.1",
    "rollup-plugin-visualizer": "^5.14.0",
    "terser": "^5.43.1",
    "vite": "^5.0.8"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
